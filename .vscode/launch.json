{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/examples/graph/dining_philosophers/dining_philosophers",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/examples/graph/dining_philosophers",
            "environment": [{"name": "LD_LIBRARY_PATH","value": "${workspaceFolder}/build/linux_intel64_gcc_cc5.4.0_libc2.23_kernel4.15.0_debug"}],
            "externalConsole": false,
            "MIMode": "gdb",
            "additionalSOLibSearchPath": "${workspaceFolder}/build/linux_intel64_gcc_cc5.4.0_libc2.23_kernel4.15.0_debug",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}